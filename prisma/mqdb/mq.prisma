generator client {
  provider        = "prisma-client-js"
  output = "@mqdb"
  binaryTargets = "rhel-openssl-1.0.x"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_MQ")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}